    "concurrently": "^7.4.0",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.2.0"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0"
  }
